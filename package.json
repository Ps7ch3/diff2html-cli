{
  "name": "diff2html-cli",
  "version": "4.2.1",
  "homepage": "https://www.github.com/rtfpessoa/diff2html-cli",
  "description": "Fast Diff to colorized HTML",
  "keywords": [
    "git",
    "diff",
    "pretty",
    "side",
    "line",
    "side-by-side",
    "line-by-line",
    "character",
    "highlight",
    "pretty",
    "color",
    "html",
    "diff2html",
    "difftohtml",
    "colorized"
  ],
  "author": {
    "name": "Rodrigo Fernandes",
    "email": "rtfrodrigo@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/rtfpessoa/diff2html-cli.git"
  },
  "bugs": {
    "url": "https://www.github.com/rtfpessoa/diff2html-cli/issues"
  },
  "engines": {
    "node": "8.* || >=10"
  },
  "preferGlobal": true,
  "scripts": {
    "prepare": "yarn run build",
    "build": "tsc",
    "lint": "eslint '*/**/*.{js,jsx,ts,tsx}'",
    "style": "yarn run lint",
    "test": "jest",
    "coverage": "jest --collectCoverage",
    "coverage-html": "yarn run coverage && open ./coverage/index.html",
    "codacy": "cat ./coverage/lcov.info | codacy-coverage",
    "preversion": "yarn run test",
    "postversion": "git push && git push --tags"
  },
  "bin": {
    "diff2html": "./bin/diff2html"
  },
  "main": "./build/main.js",
  "dependencies": {
    "clipboardy": "^2.1.0",
    "diff2html": "^2.12.1",
    "open": "^6.4.0",
    "request": "^2.88.0",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/node": "^13.1.2",
    "@types/request": "2.48.4",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.14.0",
    "codacy-coverage": "^3.4.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-config-standard": "14.0.1",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jest": "23.2.0",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "ts-jest": "24.2.0",
    "typescript": "^3.5.3"
  },
  "license": "MIT",
  "files": [
    "bin",
    "build",
    "template.html"
  ]
}
